{
  "expo": {
    "name": "invergordon-bar-app",
    "slug": "invergordon-bar-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#25245b"
    },
    "updates": {
