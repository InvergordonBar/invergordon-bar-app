    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.invergordon.barapp",
      "newArchEnabled": true
    },
    "android": {
      "package": "com.invergordon.barapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#25245b"
      },
      "newArchEnabled": true,
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f3fe51e4-ce89-4ecb-a037-6caaaaa09435"
      }
    }
  }
}
